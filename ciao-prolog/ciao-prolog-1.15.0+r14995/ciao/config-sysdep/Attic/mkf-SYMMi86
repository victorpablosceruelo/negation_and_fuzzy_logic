# -*- mode: Makefile; -*-
FOREIGN_FILES_FLAG=

#  Local C compiler
LCC=$(CC)

# C compiler
CC=cc

# Linker
LD=ld

ARCHNAME=i86

OSNAME=Dynix

# Symmetry C compiler and linker specific options
SDBEGIN="-ZO200000"
LIBS=-lseq -lpps
OPTIM_FLAG=-i

# Operating system version
OSREV=DYNIX

# How to install an make directories
MKDIR=mkdir
INSTALL="install -c"

DEFAULTYPE=dyn
