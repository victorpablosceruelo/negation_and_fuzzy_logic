CIAOBUILDDIRtemp=../../../../build
include $(CIAOBUILDDIRtemp)/CONFIG_mkf

compile:
	$(SETLOCALCIAO) $(CIAOC) -x phone_book
	$(SETLOCALCIAO) $(CIAOC) -x tcl_calculator
	$(SETLOCALCIAO) $(CIAOC) -x tcl_factorial
	$(SETLOCALCIAO) $(CIAOC) -x tcl_queens
	$(SETLOCALCIAO) $(CIAOC) -x canvas

clean:
	rm -f *.itf *.asr *.po

distclean: clean
	rm -f *~ phone_book queens tcl_calculator tcl_factorial canvas
